      "taskType": "type_mismatch", 
      "patternData": {
        "problem": "TypeError: 'NoneType' object is not callable",
        "code": "result = user.get_profile().format_name()",
        "solution": "profile = user.get_profile(); result = profile.format_name() if profile else ''",
        "explanation": "Check for None before method calls and handle gracefully"
      },
      "successRate": 0.88,
      "usageCount": 32,
      "tags": ["python", "type", "none_check"],
      "environment": {
