      "taskType": "null_reference",
      "patternData": {
        "problem": "Cannot read property 'substring' of null",
        "code": "user.profile.name.substring(0, 10)",
        "solution": "user.profile.name?.substring(0, 10) || ''",
        "explanation": "Use optional chaining and null coalescing for safe property access"
      },
